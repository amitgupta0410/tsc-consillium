
@{
    ViewBag.Title = "LeadsDashboard";
}

<div class="row">
    <div class="col-md-12">
        <h3 class="page-title">
            View all Lead/Search/Assign
        </h3>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-md-12">
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Keyword</b></label><br>
                <input class="form-control" id="Keyword" name="Keyword" type="text" value="">
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Industry</b></label><br>
                <select class="form-control">
                    <option>Option1</option>
                    <option>Option2</option>
                </select>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Geography</b></label><br>
                <select class="form-control">
                    <option>Option1</option>
                    <option>Option2</option>
                </select>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Project Name</b></label><br>
                <input class="form-control" id="Keyword" name="Keyword" type="text" value="">
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Company</b></label><br>
                <input class="form-control" id="Keyword" name="Keyword" type="text" value="">
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-1">
            <div class="form-group">
                <input type="submit" value="Filter" class="btn btn-primary">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <input type="submit" value="Export To Excel" class="btn btn-primary">
            </div>
        </div>
    </div>
</div>
<br />
<div class="row">
    <div class="col-lg-12">
        <div class="col-md-9">
            <div class="table-responsive table-bordered">
                <table class="table">
                    <tbody>
                        <tr>
                            <th style="text-align:center;">
                                <input type="checkbox" name="AllCheckBox" value="checked" />
                            </th>
                            <th><label>Name</label></th>
                            <th><label>Current Company</label></th>
                            <th><label>E-Mail-Id</label></th>
                            <th><label>Industry</label></th>
                            <th><label>Geography</label></th>
                            
</tr>
                        <tr>
                            <td style="text-align:center;"><input type="checkbox" name="AllCheckBox" value="checked" /></td>
                            <td>John Smith</td>
                            <td>WHEELS INC</td>
                            <td>WHEELS INC</td>
                            <td></td>
                            <td></td>
                            <td><input type="submit" class="btn btn-default" value="View Details" /></td>
                        </tr>
                        <tr>
                            <td style="text-align:center;"><input type="checkbox" name="CheckBoxEdit" value="checked" /></td>
                            <td>John Smith</td>
                            <td>DOORS INC</td>
                            <td>DOORS INC</td>
                            <td></td>
                            <td></td>
                            <td><input type="submit" class="btn btn-default" value="View Details" /></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="clearfix"></div>
        <br />
        <div class="col-md-2">
            <div class="form-group">
                <select class="form-control">
                    <option>Value1</option>
                    <option>Value2</option>
                    <option>Value3</option>
                </select>
            </div>
        </div>
        <div class="col-md-1">
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary">
            </div>
        </div>
    </div>
</div>
