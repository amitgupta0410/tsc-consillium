                <label><b>Project Lead</b></label><br>
                <select class="form-control">
                    <option>Option1</option>
                    <option>Option2</option>
                </select>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Status</b></label>
                <select class="form-control">
                    <option>Option1</option>
                    <option>Option2</option>
                </select>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label><b>Assigned To</b></label>
                <select class="form-control">
                    <option>Option1</option>
                    <option>Option2</option>
                </select>
            </div>
        </div>
